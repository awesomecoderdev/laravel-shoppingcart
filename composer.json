{
	"name": "awesomecoder/shoppingcart",
	"description": "AwesomeCoder Laravel Shoppingcart",
	"keywords": [
		"awesomeCoder",
		"laravel",
		"shoppingcart"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Mohammad Ibrahim",
			"email": "awesomecoder.dev@gmail.com"
		}
	],
	"require": {
		"illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*|| 5.6.* || 5.7.*",
		"illuminate/session": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*|| 5.6.* || 5.7.*",
		"illuminate/events": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*|| 5.6.* || 5.7.*"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.3.3",
		"mockery/mockery": "dev-master",
		"orchestra/testbench": "7.x"
	},
	"autoload": {
		"psr-4": {
			"AwesomeCoder\\Shoppingcart\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"AwesomeCoder\\Tests\\Shoppingcart\\": "tests/"
		}
	},
	"minimum-stability": "stable",
	"extra": {
		"laravel": {
			"providers": [
				"AwesomeCoder\\Shoppingcart\\ShoppingcartServiceProvider"
			],
			"aliases": {
				"Cart": "AwesomeCoder\\Shoppingcart\\Facades\\Cart"
			}
		}
	}
}
